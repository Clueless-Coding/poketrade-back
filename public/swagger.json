  "info": {
    "title": "Poketrade API",
    "description": "",
    "version": "1.0.0",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "securitySchemes": {
      "AccessToken": {
        "type": "apiKey",
        "in": "header",
        "name": "x-access-token"
      }
    },
    "schemas": {
      "LoginInputDTO": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string"
          },
          "password": {
            "type": "string"
          }
        },
        "required": [
          "username",
          "password"
        ]
      },
      "LoginOutputDTO": {
        "type": "object",
        "properties": {
          "accessToken": {
            "type": "string"
          }
        },
        "required": [
          "accessToken"
        ]
      },
      "RegisterInputDTO": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string"
          },
          "password": {
            "type": "string"
          },
          "confirmPassword": {
            "type": "string"
          }
        },
        "required": [
          "username",
          "password",
          "confirmPassword"
        ]
      },
      "RegisterOutputDTO": {
        "type": "object",
        "properties": {
          "accessToken": {
            "type": "string"
          }
        },
        "required": [
          "accessToken"
        ]
      },
      "UserOutputDTO": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "balance": {
            "type": "number"
          }
        },
        "required": [
          "id",
          "name",
          "balance"
        ]
      },
      "PokemonOutputDTO": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number"
          },
          "name": {
            "type": "string"
          },
          "worth": {
            "type": "number"
          },
          "height": {
            "type": "number"
          },
          "weight": {
            "type": "number"
          },
          "image": {
            "type": "string"
          }
        },
        "required": [
          "id",
          "name",
          "worth",
          "height",
          "weight",
          "image"
        ]
      },
      "UserInventoryEntryOutputDTO": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "receivedAt": {
            "format": "date-time",
            "type": "string"
          },
          "pokemon": {
            "$ref": "#/components/schemas/PokemonOutputDTO"
          }
        },
        "required": [
          "id",
          "receivedAt",
          "pokemon"
        ]
      },
      "PackOutputDTO": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "price": {
            "type": "number"
          },
          "image": {
            "type": "string"
          }
        },
        "required": [
          "id",
          "name",
          "description",
          "price",
          "image"
        ]
      },
      "PackWithPokemonsOutputDTO": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "price": {
            "type": "number"
          },
          "image": {
            "type": "string"
          },
          "pokemons": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/PokemonOutputDTO"
            }
          }
        },
        "required": [
          "id",
          "name",
          "description",
          "price",
          "image",
          "pokemons"
        ]
      },
      "OpenedPackOutputDTO": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "openedAt": {
            "format": "date-time",
            "type": "string"
          },
          "user": {
            "$ref": "#/components/schemas/UserOutputDTO"
          },
          "pack": {
            "$ref": "#/components/schemas/PackOutputDTO"
          },
          "pokemon": {
            "$ref": "#/components/schemas/PokemonOutputDTO"
          }
        },
        "required": [
          "id",
          "openedAt",
          "user",
          "pack",
          "pokemon"
        ]
      }
    }
  }
}