    "/api/users/me/inventory": {
      "get": {
        "operationId": "UsersController_getMeInventory",
        "parameters": [
          {
            "name": "page",
            "required": false,
            "in": "query",
            "schema": {
              "default": 1,
              "type": "number"
            }
          },
          {
            "name": "limit",
            "required": false,
            "in": "query",
            "schema": {
              "default": 10,
              "type": "number"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "items": {
                      "type": "array",
                      "items": {
                        "$ref": "#/components/schemas/UserInventoryEntryOutputDTO"
                      }
                    },
                    "meta": {
                      "type": "object",
                      "properties": {
                        "totalItems": {
                          "type": "number"
                        },
                        "itemCount": {
                          "type": "number"
                        },
                        "itemsPerPage": {
                          "type": "number"
                        },
                        "totalPages": {
                          "type": "number"
                        },
                        "currentPage": {
                          "type": "number"
                        }
                      },
                      "required": [
                        "totalItems",
                        "itemCount",
                        "itemsPerPage",
                        "totalPages",
                        "currentPage"
                      ]
                    }
                  },
                  "required": [
                    "items",
                    "meta"
                  ]
                }
              }
            }
          }
        },
        "tags": [
          "Users"
        ],
        "security": [
          {
            "AccessToken": []
          }
        ]
      }
    },
